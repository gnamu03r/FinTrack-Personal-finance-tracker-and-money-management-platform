{
  "version": 2,
  "builds": [
    {
      "src": "build/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/(.*)",
      "dest": "http://https://finbackend.vercel.app/api/$1" 
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}
